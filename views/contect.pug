extends base.pug

block scripts
   script(src="/static/index.js") 
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js", integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p", crossorigin="anonymous") 
block style 
   style 
      include ../static/style.css
      include ../static/contectStyle.css
      link( href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3", crossorigin="anonymous")
      

block contect 
   h1 Contect us
   form(action="/contect", method="post")
      div.container
        input(type="text",  class="formInput" name="name",placeholder="Enter your name") 
        input(type="email", class="formInput" name="email",placeholder="Enter your Email id")
        input(type="number",class="formInput" name="number",placeholder="Enter your Number")
        input(type="text",  class="formInput" name="address",placeholder="Enter your Address")
        input(type="text",  class="formInput" name="password",placeholder="Enter your Password")
        button 
          |submit

   
         
